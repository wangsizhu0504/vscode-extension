    "editorGutter.foldingControlForeground": "#dedcd590",

    // Diff editor colors
		"diffEditor.insertedTextBackground": "#66b39530",
		"diffEditor.removedTextBackground": "#ff878730",
		"diffEditor.border": "#ffffff1a",
		"diffEditor.diagonalFill": "#ffffff1a",
		"diffEditor.insertedLineBackground": "#66b39530",
		"diffEditor.removedLineBackground": "#ff878730",

   // Editor widget colors
		"editorWidget.background": "#2a2a2a",
		"editorSuggestWidget.background": "#202020",
		"editorSuggestWidget.selectedBackground": "#333333",
		"editorStickyScrollHover.background": "#333333",

		// Peek view colors
		"peekView.border": "#666666",
		"peekViewEditor.background": "#333333",
		"peekViewEditor.matchHighlightBackground": "#66b3953f",
		"peekViewEditor.matchHighlightBorder": "#66b395",
		"peekViewResult.matchHighlightBackground": "#66b3953f",
		"peekViewResult.selectionBackground": "#66b3953f",
		"peekViewTitle.background": "#333333",

    // Merge conflicts colors
    "merge.currentHeaderBackground": "#66b3957f",
    "merge.currentContentBackground": "#66b3953f",
    "merge.incomingHeaderBackground": "#7098d47f",
    "merge.incomingContentBackground": "#7098d43f",

    // Panel colors
    "panel.background": "#191919",

		// Status bar colors
		"statusBar.background": "#222222",
		"statusBar.foreground": "#999999",
		"statusBar.border": "#ffffff1a",
		"statusBar.debuggingBackground": "#ff8787",
		"statusBar.debuggingForeground": "#f5e6e6",
		"statusBar.noFolderBackground": "#222222",
		"statusBar.noFolderForeground": "#999999",
		"statusBar.focusBorder": "#00000000",
		"statusBarItem.prominentBackground": "#66b395",
		"statusBarItem.prominentForeground": "#000000",
		"statusBarItem.remoteBackground": "#66b395",
		"statusBarItem.remoteForeground": "#000000",
		"statusBarItem.errorBackground": "#ff4d4f",
		"statusBarItem.errorForeground": "#f5e6e6",
		"statusBarItem.warningBackground": "#faad14",
		"statusBarItem.warningForeground": "#000000",

    // Menu bar colors
    "menu.foreground": "#cccccc",
    "menu.background": "#191919",
    
		// Command Center colors
		"commandCenter.activeBackground": "#ffffff10",
		"commandCenter.border": "#ffffff2a",

    // Banner colors
    "banner.foreground": "#ffffff",
    "banner.background": "#444444",

    // Quick picker colors
    "pickerGroup.foreground": "#66b395",
    "pickerGroup.border": "#444d56",
    "quickInput.background": "#191919",

    // Integrated Terminal colors
		"terminal.background": "#191919",
		"terminal.ansiBlack": "#1e1e1e",
		"terminal.ansiBlue": "#096dd9",
		"terminal.ansiBrightBlack": "#1e1e1e",
		"terminal.ansiBrightBlue": "#096dd9",
		"terminal.ansiBrightCyan": "#75e4e4",
		"terminal.ansiBrightGreen": "#6ec145",
		"terminal.ansiBrightMagenta": "#b479c3",
		"terminal.ansiBrightRed": "#ff4d4f",
		"terminal.ansiBrightWhite": "#eeeeee",
		"terminal.ansiBrightYellow": "#e2c97e",
		"terminal.ansiCyan": "#75e4e4",
		"terminal.ansiGreen": "#6ec145",
		"terminal.ansiMagenta": "#b479c3",
		"terminal.ansiRed": "#ff4d4f",
		"terminal.ansiWhite": "#f4f5f6",
		"terminal.ansiYellow": "#e2c97e",
		"terminal.selectionBackground": "#444444",
		"terminal.tab.activeBorder": "#6ec145",

    // Breadcrumbs colors
    "breadcrumb.foreground": "#666666",
    "breadcrumb.focusForeground": "#dbd7caee",
    "breadcrumb.activeSelectionForeground": "#eeeeee15",
    "breadcrumbPicker.background": "#191a1d",

    "errorForeground": "#e62d2d",
    "textBlockQuote.background": "#191a1d",
    "textBlockQuote.border": "#252525",
    "textPreformat.foreground": "#d1d5da",
    "textSeparator.foreground": "#586069",
    "checkbox.background": "#292929",
    "checkbox.border": "#2f363d",

    "titleBar.border": "#292929",
    "extensionButton.prominentForeground": "#f6f6f4",
    "extensionButton.prominentBackground": "#42b883",
    "extensionButton.prominentHoverBackground": "#747bff",
    "tree.indentGuidesStroke": "#2f363d",
    "notificationCenterHeader.foreground": "#959da5",
    "notificationCenterHeader.background": "#191a1d",
    "notifications.foreground": "#dbd7caee",
    "notifications.background": "#191a1d",
    "notifications.border": "#252525",
    "notificationsErrorIcon.foreground": "#ff7875",
    "notificationsWarningIcon.foreground": "#ffc53d",
    "notificationsInfoIcon.foreground": "#69c0ff",

    "statusBarItem.prominentHoverBackground": "#FFB86C",

    "editorBracketMatch.background": "#42b88320",

    "editorOverviewRuler.border": "#111",
    "panel.border": "#42b883",
    "panelTitle.activeBorder": "#42b883",
    "panelTitle.activeForeground": "#f6f6f4",
    "panelTitle.inactiveForeground": "#7b7f8b",
    "panelInput.border": "#2f363d",
    

    "gitDecoration.addedResourceForeground": "#2fa578",
    "gitDecoration.modifiedResourceForeground": "#d4bc6c",
    "gitDecoration.deletedResourceForeground": "#cb7676",
    "gitDecoration.untrackedResourceForeground": "#2fa578",
    "gitDecoration.ignoredResourceForeground": "#515c5b",
    "gitDecoration.conflictingResourceForeground": "#d4976c",
    "gitDecoration.submoduleResourceForeground": "#dedcd590",

    "peekViewResult.background": "#191a1d",
    "settings.headerForeground": "#dbd7caee",
    "settings.modifiedItemIndicator": "#42b883",
    "problemsErrorIcon.foreground": "#cb7676",
    "problemsWarningIcon.foreground": "#d4976c",
    "problemsInfoIcon.foreground": "#6394bf",
    "editorHint.foreground": "#42b883",
    "editor.stackFrameHighlightBackground": "#a707",
    "editor.focusedStackFrameHighlightBackground": "#b808",

  },
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#db889a",
    "property": "#b8a965",
    "interface": "#5DA994",
    "type": "#5DA994",
    "class": "#647eff"
  },
  "tokenColors": [
    {
      "name": "Comments(注释)",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#ffffff4f"
      }
    },
    {
      "name": "Variables(变量)",
      "scope": [
        "variable",
				"string constant.other.placeholder",
				"variable.parameter",
      ],
      "settings": {
        "foreground": "#f0cda2"
      }
    },
    {
      "name": "object properties(对象属性)",
      "scope": [
        "support.variable.property",
        "meta.object.member.tsx"
      ],
      "settings": {
        "foreground": "#738df8"
      }
    },
    {
      "name": "Block Level Variables(块级变量)",
      "scope": [
        "meta.block variable.other"
      ],
      "settings": {
        "foreground": "#d19a66"
      }
    },  
    {
      "name": "Other Variable, String Link(其他变量,字符链接)",
      "scope": [
        "support.other.variable",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#ff9cdb"
      }
    },
    {
      "name": "Boolean & Number",
      "scope": [
        "number",
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.undefined.js",
        "constant.language.null.js",
        "constant.language.undefined.ts",
        "constant.language.null.ts",
        "support.type.primitive.ts",
        "support.type.builtin.ts",
        "keyword.operator.glob.shell"
      ],
      "settings": {
        "foreground": "#42b883"
      }
    },
    {
      "name": "Constants(常数)",
      "scope": [
        "constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#bf9eee"
      }
    },
    {
      "name": "Constant escape sequences",
      "scope": [
        "constant.character.escape",
        "constant.character.string.escape",
        "constant.regexp"
      ],
      "settings": {
        "foreground": "#f286c4"
      }
    },
    {
      "name": "String",
      "scope": [
        "meta.template.expression.js",
        "meta.template.expression.ts",
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition",
        "constant.other.reference.link",
        "string.other.link",
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown",
        "string.quoted",
        "string",
        "string punctuation.section.embedded source"
      ],
      "settings": {
        "foreground": "#70c8a6"
      }
    },
    {
      "name": "Properties(ts文件中的属性)",
      "scope": ["variable.other.property.ts"],
      "settings": {
        "foreground": "#ff8d47"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#999"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "keyword.primitive-datatypes.swift",
        "keyword.type.cs",
        "meta.protocol-list.objc",
        "meta.return-type.objc",
        "source.go storage.type",
        "source.groovy storage.type",
        "source.java storage.type",
        "source.powershell entity.other.attribute-name",
        "storage.class.std.rust",
        "storage.type.attribute.swift",
        "storage.type.c",
        "storage.type.core.rust",
        "storage.type.cs",
        "storage.type.groovy",
        "storage.type.objc",
        "storage.type.php",
        "storage.type.haskell",
        "storage.type.ocaml"
      ],
      "settings": {
        "foreground": "#d19a66"
      }
    },
    {
      "name": "RegExp string",
      "scope": [
        "string.regexp",
        "constant.other.character-class.set.regexp",
        "constant.character.escape.backslash.regexp"
      ],
      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "RegExp start and end characters",
      "scope": [
        "string.regexp punctuation.definition.string.begin",
        "string.regexp punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#ee6666"
      }
    },
    {
      "name": "Keyword, Storage(关键字)",
      "scope": [
        "keyword",
        "storage.type.class.jsdoc",
        "punctuation.definition.keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#d4a7f4"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation.separator.inheritance.php",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#c678dd"
      }
    },
    {
			"name": "Tag(标签)",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#1AB58E"
			}
		},
    {
      "name": "Function Name(函数名)",
      "scope": [
        "entity.name.function",
        "meta.function-call.object",
        "meta.function-call.php",
        "meta.function-call.static",
        "meta.method-call.java meta.method",
        "meta.method.groovy",
        "support.function.any-method.lua",
        "keyword.operator.function.infix"
      ],
      "settings": {
        "foreground": "#389bed"
      }
    },
    {
      "name": "Function parameters(函数参数)",
      "scope": [
        "entity.name.variable.parameter",
        "meta.at-rule.function variable",
        "meta.at-rule.mixin variable",
        "meta.function.arguments variable.other.php",
        "meta.selectionset.graphql meta.arguments.graphql variable.arguments.graphql",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#f0cda2"
      }
    },
    {
      "name": "JSDoc-style keywords",
      "scope": [
        "comment keyword.codetag.notation",
        "comment.block.documentation keyword",
        "comment.block.documentation storage.type.class"
      ],
      "settings": {
        "foreground": "#f286c4"
      }
    },
    {
      "name": "JSDoc-style types",
      "scope": [
        "comment.block.documentation entity.name.type"
      ],
      "settings": {
        "foreground": "#97e1f1"
      }
    },
    {
      "name": "JSDoc-style type brackets",
      "scope": [
        "comment.block.documentation entity.name.type punctuation.definition.bracket"
      ],
      "settings": {
        "foreground": "#97e1f1"
      }
    },
    {
      "name": "JSDoc-style comment parameters",
      "scope": [
        "comment.block.documentation variable"
      ],
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter"
      ],
      "settings": {
        "foreground": "#d4a7f4"
      }
    },
    {
      "name": "User-defined class names",
      "scope": [
        "entity.name.type.class",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#d19a66"
      }
    },
    {
      "name": "Inherited classes(继承的类)",
      "scope": [
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#42d392",
      }
    },
    {
      "name": "this, super, self, etc.",
      "scope": [
        "keyword.expressions-and-types.swift",
        "keyword.other.this",
        "variable.language",
        "variable.language punctuation.definition.variable.php",
        "variable.other.readwrite.instance.ruby",
        "variable.parameter.function.language.special"
      ],
      "settings": {
        "foreground": "#40a9ff"
      }
    },
    {
      "name": "async await",
      "scope": [
        "storage.modifier.async",
        "keyword.control.flow"
      ],
      "settings": {
        "foreground": "#fa424c",
        "fontStyle": "bold",
      }
    },
    {
      "name": "Docstrings",
      "scope": [
        "string.quoted.docstring.multi",
        "string.quoted.docstring.multi.python punctuation.definition.string.begin",
        "string.quoted.docstring.multi.python punctuation.definition.string.end",
        "string.quoted.docstring.multi.python constant.character.escape"
      ],
      "settings": {
        "foreground": "#7b7f8b"
      }
    },
    {
      "name": "Dates / timestamps (serializable languages)",
      "scope": [
        "constant.other.date",
        "constant.other.timestamp"
      ],
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "name": "[JSON]-JSON Support Constants",
      "scope": ["support.constant.json","support.type.property-name.json"],
      "settings": {
        "foreground": "#db7cd7"
      }
    },
    {
      "name": "[JSON]-JSON Property values (number)",
      "scope": "meta.structure.dictionary.value.json constant.numeric.json",
      "settings": {
        "foreground": "#25aae2"
      }
    },
    {
      "name": "[JSON]-JSON Property values (string)",
      "scope": "meta.structure.dictionary.value.json string.quoted.double",
      "settings": {
        "foreground": "#42d392"
      }
    },
    {
      "name": "[JSON]-Strings in JSON values",
      "scope": "string.quoted.double.json punctuation.definition.string.json",
      "settings": {
        "foreground": "#c776dc"
      }
    },
    {
      "name": "[JSON]- Specific JSON Property values like null",
      "scope": "meta.structure.dictionary.json meta.structure.dictionary.value constant.language",
      "settings": {
        "foreground": "#f1592a"
      }
    },
    {
      "name": "Destructuring / aliasing reference name (LHS)",
      "scope": [
        "meta.import variable.other.readwrite",
        "meta.variable.assignment.destructured.object.coffee variable"
      ],
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "name": "Destructuring / aliasing variable name (RHS)",
      "scope": [
        "meta.import variable.other.readwrite.alias",
        "meta.export variable.other.readwrite.alias",
        "meta.variable.assignment.destructured.object.coffee variable variable"
      ],
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "name": "[HTML] - Entity Name (html标签)",
      "scope": "text.html.basic entity.name",
      "settings": {
        "foreground": "#9effff"
      }
    },
    {
      "name": "[HTML] - ID value (html id 值)",
      "scope": "meta.toc-list.id.html",
      "settings": {
        "foreground": "#A5FF90"
      }
    },
    {
      "name": "[HTML] - Entity Other",
      "scope": "text.html.basic,entity.other",
      "settings": {
        "foreground": "#ffc600"
      }
    },
    {
      "name": "[HTML] - Quotes. these are a slightly different colour because expand selection will then not include quotes",
      "scope": "punctuation.definition.string.begin, punctuation.definition.string.end",
      "settings": {
        "foreground": "#8fbaf6"
      }
    },
    {
      "name": "Entity Types",
      "scope": [ "entity.name.section.vue"],
      "settings": {
        "foreground": "#13e48d",
        "fontStyle": "bold"
      }
    },
    // CSS
    {
      "name": "[CSS] - Entity",
      "scope": ["source.css entity", "source.stylus entity"],
      "settings": {
        "foreground": "#3ad900"
      }
    },
