{
	"name": "wsz-vscode",
	"displayName": "wsz-vscode",
	"description": "web开发工具集合(自用)",
	"publisher": "wangsizhu1996",
	"version": "1.0.3",
	"engines": {
		"vscode": "^1.63.0"
	},
	"repository": {
		"type": "git",
		"url": "https://gitee.com/wangsizhu0504/wsz-vscode-plugin.git"
	},
	"categories": [
		"Snippets",
		"Other"
	],
	"icon": "taurus.png",
	"activationEvents": [
		"*",
		"onLanguage:html",
		"onLanguage:vue",
		"onCommand:wsz-vscode.wszfileheader",
		"onCommand:wsz-vscode.wszfilecursorTip",
		"onCommand:wsz-vscode.displayLogMessage",
		"onCommand:wsz-vscode.commentAllLogMessages",
		"onCommand:wsz-vscode.uncommentAllLogMessages",
		"onCommand:wsz-vscode.deleteAllLogMessages"
	],
	"main": "./extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/element.json"
			},
			{
				"language": "javascript",
				"path": "./snippets/antdv.json"
			},
			{
				"language": "javascript",
				"path": "./snippets/vue.json"
			},
			{
				"language": "vue-html",
				"path": "./snippets/antdv-html.json"
			},
			{
				"language": "vue-html",
				"path": "./snippets/element-html.json"
			},
			{
				"language": "vue-html",
				"path": "./snippets/vue-html.json"
			}
		],
		"configuration": {
			"type": "object",
			"title": "配置文件",
			"properties": {
				"wsz-vscode.customMade": {
					"type": "object",
					"default": {},
					"description": "头部注释,默认设置：{'Author':'your name','Date':'Do not edit','LastEditors':'your name','LastEditTime':'Do not edit','Description':'file content'}"
				},
				"wsz-vscode.cursorMode": {
					"type": "object",
					"default": {},
					"description": "在光标处插入函数注释,默认配置:{'description':'','param':'','return':''}"
				},
				"wsz-vscode.configObj": {
					"type": "object",
					"default": {
						"createFileTime": true,
						"language": {
							"languagetest": {
								"head": "/$$",
								"middle": " $ @",
								"end": " $/",
								"functionSymbol": {
									"head": "/** ",
									"middle": " * @",
									"end": " */"
								},
								"functionParams": "js"
							}
						},
						"autoAdd": true,
						"autoAddLine": 100,
						"autoAlready": true,
						"annotationStr": {
							"head": "/*",
							"middle": " * @",
							"end": " */",
							"use": false
						},
						"headInsertLine": {
							"php": 2,
							"sh": 2
						},
						"beforeAnnotation": {
							"文件后缀": "该文件后缀的头部注释之前添加某些内容"
						},
						"afterAnnotation": {
							"文件后缀": "该文件后缀的头部注释之后添加某些内容"
						},
						"specialOptions": {
							"特殊字段": "自定义比如LastEditTime/LastEditors"
						},
						"switch": {
							"newlineAddAnnotation": true
						},
						"supportAutoLanguage": [],
						"prohibitAutoAdd": [
							"json"
						],
						"folderBlacklist": [
							"node_modules",
							"文件夹禁止自动添加头部注释"
						],
						"prohibitItemAutoAdd": [
							"项目的全称, 整个项目禁止自动添加头部注释, 可以使用快捷键添加"
						],
						"moveCursor": true,
						"dateFormat": "YYYY-MM-DD HH:mm:ss",
						"atSymbol": [
							"@",
							"@"
						],
						"atSymbolObj": {
							"文件后缀": [
								"头部注释@符号",
								"函数注释@符号"
							]
						},
						"colon": [
							": ",
							": "
						],
						"colonObj": {
							"文件后缀": [
								"头部注释冒号",
								"函数注释冒号"
							]
						},
						"filePathColon": "路径分隔符替换",
						"showErrorMessage": false,
						"writeLog": false,
						"wideSame": false,
						"wideNum": 13,
						"functionWideNum": 0,
						"CheckFileChange": false,
						"createHeader": true,
						"useWorker": false,
						"designAddHead": false,
						"headDesignName": "random",
						"headDesign": false,
						"cursorModeInternalAll": {},
						"openFunctionParamsCheck": true,
						"functionParamsShape": [
							"{",
							"}"
						],
						"functionBlankSpaceAll": {},
						"functionTypeSymbol": "*",
						"typeParamOrder": "type param",
						"customHasHeadEnd": {},
						"throttleTime": 60000
					},
					"description": "插件配置选项,readme:https://github.com/OBKoro1/koro1FileHeader/wiki/%E9%85%8D%E7%BD%AE"
				},
				"wsz-vscode.wrapLogMessage": {
					"type": "boolean",
					"default": false,
					"description": "确定是否应该对日志消息进行包装."
				},
				"wsz-vscode.logMessagePrefix": {
					"type": "string",
					"default": "🚀",
					"description": "日志输出前缀."
				},
				"wsz-vscode.addSemicolonInTheEnd": {
					"type": "boolean",
					"default": false,
					"description": "是否在日志消息的末尾添加分号."
				},
				"wsz-vscode.insertEnclosingClass": {
					"type": "boolean",
					"default": true,
					"description": "是否在日志消息中插入所选变量的外围类."
				},
				"wsz-vscode.insertEnclosingFunction": {
					"type": "boolean",
					"default": true,
					"description": "是否在日志消息中插入所选变量的封闭函数."
				},
				"wsz-vscode.delimiterInsideMessage": {
					"type": "string",
					"default": "~",
					"description": "分隔不同日志消息元素(文件名、行号、类、函数和变量)的分隔符"
				},
				"wsz-vscode.quote": {
					"type": "string",
					"enum": [
						"\"",
						"'",
						"`"
					],
					"default": "`",
					"description": "双引号，单引号或反引号"
				},
				"wsz-vscode.webview": {
					"type": "object",
					"default": {
						"Vue": "https://cn.vuejs.org/v2/guide/",
						"Vuex": "https://vuex.vuejs.org/zh/",
						"Vue Router": "https://router.vuejs.org/zh/",
						"Vue SSR": "https://ssr.vuejs.org/zh/",
						"Nuxt.js": "https://zh.nuxtjs.org/",
						"Vue CLI 3": "https://cli.vuejs.org/zh/",
						"VuePress": "https://vuepress.vuejs.org/zh/"
					},
					"description": "双引号，单引号或反引号"
				}
			}
		},
		"menus": {
			"editor/context": [
				{
					"command": "wsz-vscode.displayLogMessage",
					"when": "editorHasSelection",
					"group": "navigation@2"
				},
				{
					"command": "wsz-vscode.commentAllLogMessages",
					"when": "editorFocus",
					"group": "navigation@3"
				},
				{
					"command": "wsz-vscode.deleteAllLogMessages",
					"when": "editorFocus",
					"group": "navigation@4"
				},
				{
					"command": "wsz-vscode.wszfileheader",
					"when": "editorFocus",
					"group": "navigation@5"
				},
				{
					"command": "wsz-vscode.wszfilecursorTip",
					"when": "editorFocus",
					"group": "navigation@6"
				}
			]
		},
		"commands": [
			{
				"command": "wsz-vscode.wszfileheader",
				"title": "文件头部注释"
			},
			{
				"command": "wsz-vscode.wszfilecursorTip",
				"title": "函数注释"
			},
			{
				"command": "wsz-vscode.errPathSet",
				"title": "设置报错日志储存",
				"category": "errPath"
			},
			{
				"command": "wsz-vscode.displayLogMessage",
				"title": "输出 console log "
			},
			{
				"command": "wsz-vscode.commentAllLogMessages",
				"title": "注释 console log "
			},
			{
				"command": "wsz-vscode.uncommentAllLogMessages",
				"title": "解除console log注释"
			},
			{
				"command": "wsz-vscode.deleteAllLogMessages",
				"title": "删除console log"
			},
			{
				"command": "wsz-vscode.openWebview",
				"title": "百度一下"
			}
		],
		"keybindings": [
			{
				"command": "wsz-vscode.wszfileheader",
				"key": "shift+alt+1",
				"mac": "shift+alt+1",
				"when": "editorTextFocus"
			},
			{
				"command": "wsz-vscode.wszfilecursorTip",
				"key": "shift+alt+2",
				"mac": "shift+alt+2",
				"when": "editorTextFocus"
			},
			{
				"command": "wsz-vscode.displayLogMessage",
				"key": "ctrl+cmd+[",
				"mac": "ctrl+cmd+[",
				"when": "editorTextFocus"
			},
			{
				"command": "wsz-vscode.commentAllLogMessages",
				"key": "ctrl+alt+/",
				"mac": "ctrl+cmd+/"
			},
			{
				"command": "wsz-vscode.uncommentAllLogMessages",
				"key": "",
				"mac": "ctrl+alt+/"
			},
			{
				"command": "wsz-vscode.deleteAllLogMessages",
				"key": "ctrl+shift+delete",
				"mac": "ctrl+cmd+delete"
			}
		],
		"themes": [
			{
				"label": "wsz theme",
				"uiTheme": "vs-dark",
				"path": "./themes/wsz-theme.json"
			}
		]
	},
	"scripts": {
		"package": "vsce package",
		"publish": "vsce publish",
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	},
	"dependencies": {
		"cheerio": "^1.0.0-rc.1",
		"glob": "^7.1.3",
		"hoek": "^5.0.3",
		"lodash": "^4.17.15",
		"param-case": "^2.1.1",
		"pretty": "^2.0.0",
		"moment": "^2.29.1",
		"shelljs": "^0.8.3"
	}
}