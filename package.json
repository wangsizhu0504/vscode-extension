{
	"name": "wsz-vscode-plugin",
	"displayName": "wsz-vscode-plugin",
	"description": "web开发工具集合(自用)",
	"publisher":"wangsizhu",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.63.0"
	},
	"repository": {
		"type": "git",
		"url": "https://gitee.com/wangsizhu0504/wsz-vscode-plugin.git"
	},
	"categories": [
		"Snippets",
		"Other"
	],
	"icon": "logo.png",
	"activationEvents": [
		"onLanguage:html",
		"onLanguage:vue",
		"onCommand:wsz-vscode-plugin.helloWorld",
		"onCommand:wsz-vscode-plugin.deleteComplete",
		"onCommand:wsz-vscode-plugin.blockSelect"
	],
	"main": "./extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/element.json"
			},
			{
				"language": "javascript",
				"path": "./snippets/antdv.json"
			},
			{
				"language": "javascript",
				"path": "./snippets/vue.json"
			},
			{
				"language": "vue-html",
				"path": "./snippets/antdv-html.json"
			},
			{
				"language": "vue-html",
				"path": "./snippets/element-html.json"
			},
			{
				"language": "vue-html",
				"path": "./snippets/vue-html.json"
			}
		]
	},
	"scripts": {
		"package": "vsce package",
		"publish": "vsce publish",
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	},
	"dependencies": {
		"cheerio": "^1.0.0-rc.1",
		"glob": "^7.1.3",
		"hoek": "^5.0.3",
		"lodash": "^4.17.15",
		"param-case": "^2.1.1",
		"pretty": "^2.0.0",
		"shelljs": "^0.8.3"
	}
}
